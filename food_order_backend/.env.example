# Django/DB configuration example (do not commit real secrets)

# SECRET_KEY will be injected by orchestrator; falls back to default in settings if not set.
DJANGO_SECRET_KEY=your-secret-key

# Database configuration (if switching from SQLite to Postgres/MySQL later).
# If the deployment provides env vars, update settings.py accordingly to read them.
DB_ENGINE=django.db.backends.sqlite3
DB_NAME=db.sqlite3
DB_USER=
DB_PASSWORD=
DB_HOST=
DB_PORT=
